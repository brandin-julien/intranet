{% extends 'base.html.twig' %}

{% block body %}


    <h3>Liste de mes eleves:</h3>

    <ul>
        {% for user in users %}
            {% if user.roles != ["ROLE_ADMIN","ROLE_USER"] %}
                <a href="{{ path('graduationUser', { 'matter': matter.id, 'user': user.id } ) }}"><li>{{ user.username }}</li></a>
                {% set student = true %}
            {% endif %}
        {% endfor %}
    </ul>


{% endblock %}